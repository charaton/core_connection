<h3>Skill Category Name: <%= @category.name %></h3>

<%= link_to "Edit", edit_category_path(@category), :class=>"btn btn-primary", :role=>"button" %>
<%= link_to "Delete",
            category_path(@category),
            method: :delete,
            data: { confirm: "Are you sure you want to delete this category?" }, :class=>"btn btn-primary", :role=>"button" %>
