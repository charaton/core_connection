<%= simple_form_for @profile do |e| %>
  <%= e.simple_fields_for :education do |f| %>
    <%= f.input :school %>
    <%= f.input :degree %>
    <%= f.input :description %>
    <%= f.input :school_url %>
    <%= f.input :image, as: :file %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
<% end %>
